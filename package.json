{
  "name": "purescript-turbine-starter",
  "version": "0.0.1",
  "description": "Starter for PureScript and Turbine",
  "scripts": {
    "postinstall": "bower install",
    "build": "pulp browserify --to bundle.js",
    "test": "pulp test",
    "start": "concurrently \"pulp --watch build --to ./bundle.js\" \"parcel index.html\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funkia/purescript-turbine-starter.git"
  },
  "author": "Funkia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funkia/purescript-turbine-starter/issues"
  },
  "dependencies": {
    "@funkia/hareactive": "^0.3.4",
    "@funkia/turbine": "^0.4.0",
    "pulp": "^13.0.0",
    "purescript": "^0.13.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "parcel-bundler": "^1.12.4"
  }
}
